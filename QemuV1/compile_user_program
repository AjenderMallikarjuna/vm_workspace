sudo cp /home/ajender/vm_workspace/QemuV1/Hello/example/receive /home/ajender/vm_workspace/QemuV1/try/root/
sudo cp /home/ajender/vm_workspace/QemuV1/Hello/example/ecu1 /home/ajender/vm_workspace/QemuV1/try/root/
sudo cp /home/ajender/vm_workspace/QemuV1/Hello/example/ecu2 /home/ajender/vm_workspace/QemuV1/try/root/
sudo cp /home/ajender/vm_workspace/QemuV1/Hello/example/ecu3 /home/ajender/vm_workspace/QemuV1/try/root/
sudo cp /home/ajender/vm_workspace/QemuV1/Hello/basic_can/char_driver.ko /home/ajender/vm_workspace/QemuV1/try/root/
sudo cp /home/ajender/vm_workspace/QemuV1/Hello/basic_can/load /home/ajender/vm_workspace/QemuV1/try/root/
sudo cp /home/ajender/vm_workspace/QemuV1/Hello/example/simple_char /home/ajender/vm_workspace/QemuV1/try/root/
