make
cd /home/ajender/vm_workspace/QemuV1/Hello/example/
make
sudo mount -t ext2 -o rw,loop /home/ajender/vm_workspace/QemuV1/Output/rootfs_ecu1.ext2 /home/ajender/vm_workspace/QemuV1/try/
