#!/bin/sh

#export PATH="/home/ajender/QemuV1/buildroot-2023.02.4/output/host/bin:${PATH}"
#compiler_path=/home/ajender/vm_workspace/QemuV1/buildroot-2023.02.4/output/host/bin
